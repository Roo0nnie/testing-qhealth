{
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "same-origin"
				},
				{
					"key": "Cross-Origin-Embedder-Policy",
					"value": "require-corp"
				},
				{
					"key": "Cross-Origin-Resource-Policy",
					"value": "cross-origin"
				}
			]
		},
		{
			"source": "/(.*\\.wasm)",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/wasm"
				}
			]
		}
	]
}
